{
  "name": "store",
  "version": "0.1.0",
  "description": "Socketkit Store",
  "main": "src/index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config src/index.js",
    "dev": "DOTENV_CONFIG_PATH=.env.development nodemon -r dotenv/config",
    "test": "DOTENV_CONFIG_PATH=.env.development node --experimental-vm-modules -r dotenv/config node_modules/.bin/jest --detectOpenHandles",
    "lint": "eslint \"{src,apps,libs,test}/**/*.js\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.js\" --fix"
  },
  "author": "Yagiz Nizipli <yagiz@nizipli.com>",
  "dependencies": {
    "@grpc/grpc-js": "^1.2.7",
    "@grpc/proto-loader": "^0.5.6",
    "@malijs/logger": "^0.5.0",
    "app-store-scraper": "github:facundoolano/app-store-scraper",
    "consola": "^2.15.3",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "knex": "^0.21.17",
    "mali": "^0.31.0",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageProvider": "v8",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ]
  }
}
