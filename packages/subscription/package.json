{
  "name": "appstore-worker",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "DOTENV_CONFIG_PATH=.env.development nodemon -r dotenv/config",
    "test": "DOTENV_CONFIG_PATH=.env.development node --experimental-vm-modules -r dotenv/config node_modules/.bin/jest --detectOpenHandles",
    "lint": "eslint ./src/**/*.js",
    "lint:fix": "eslint --fix ./src/**/*.js",
    "seed": "node -r dotenv/config ./node_modules/.bin/knex seed:run --esm",
    "producer": "node -r dotenv/config ./src/helpers/producer.js"
  },
  "author": "Yagiz Nizipli <yagiz@nizipli.com>",
  "dependencies": {
    "@grpc/grpc-js": "^1.2.10",
    "@grpc/proto-loader": "^0.5.6",
    "@sentry/node": "^6.2.1",
    "@sentry/tracing": "^6.2.1",
    "ajv": "^7.2.1",
    "appstore-reporter": "^1.0.5",
    "consola": "^2.15.3",
    "dayjs": "^1.10.4",
    "fixer-api": "^2.2.0",
    "knex": "^0.95.1",
    "mali": "^0.40.0",
    "pg": "^8.5.1",
    "pg-range": "^1.1.0",
    "slug": "^4.0.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "uuid": "^8.3.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageProvider": "v8",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ]
  }
}
