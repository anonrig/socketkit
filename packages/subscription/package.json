  "author": "Yagiz Nizipli <yagiz@nizipli.com>",
  "dependencies": {
    "@grpc/grpc-js": "^1.3.0",
    "@grpc/proto-loader": "^0.6.1",
    "@sentry/node": "^6.3.4",
    "@sentry/tracing": "^6.3.4",
    "ajv": "^8.2.0",
    "appstore-reporter": "^1.0.5",
    "consola": "^2.15.3",
    "dayjs": "^1.10.4",
    "fixer-api": "^2.2.0",
    "knex": "^0.95.4",
    "lodash": "^4.17.21",
    "mali": "^0.41.2",
    "pg": "^8.6.0",
    "pg-range": "^1.1.0",
    "slug": "^4.0.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "dotenv": "^8.2.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "uuid": "^8.3.2"
