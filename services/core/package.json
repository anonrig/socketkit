{
  "name": "@socketkit/core-worker",
  "version": "1.0.0",
  "description": "Socketkit Core Worker",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint \"{src,apps,libs,test}/**/*.js\" --fix",
    "start": "node -r dotenv/config src/index.js",
    "dev": "DOTENV_CONFIG_PATH=.env.development nodemon -r dotenv/config"
  },
  "author": "Yagiz Nizipli <yagiz@nizipli.com>",
  "dependencies": {
    "@grpc/grpc-js": "^1.4.1",
    "@grpc/proto-loader": "^0.6.6",
    "@ory/kratos-client": "^0.7.1-alpha.2",
    "ajv": "^8.6.3",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "consola": "^2.15.3",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "fastify": "^3.22.1",
    "fastify-auth": "^1.1.0",
    "fastify-compress": "^3.6.1",
    "fastify-cookie": "^5.3.1",
    "fastify-cors": "^6.0.2",
    "fastify-helmet": "^5.3.2",
    "fastify-metrics": "^7.4.0",
    "fastify-plugin": "^3.0.0",
    "fastify-raw-body": "^3.1.0",
    "fastify-sensible": "^3.1.1",
    "knex": "^0.95.11",
    "pg": "^8.7.1",
    "qs": "^6.10.1",
    "under-pressure": "^5.8.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-security": "^1.4.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:security/recommended"
    ],
    "plugins": [
      "prettier",
      "import",
      "security"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true
      },
      "ecmaVersion": 2021
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "import/extensions": [
        "error",
        "always",
        {
          "ignorePackages": true
        }
      ]
    }
  }
}
